{
  "appName": "change-me",
  "publicWebDir": "wordpress",
  "docker": {
    "name": "change-me",
    "tag": "change-me",
    "port": "change-me"
  },
  "aws": {
    "securityGroup": "change-me",
    "ec2KeyName": "change-me",
    "ec2KeyPath": "change-me",
    "stack": "64bit Amazon Linux 2017.09 v2.6.5 running PHP 7.1",
    "instanceType": {
      "dev": "t2.micro",
      "qa": "t2.micro",
      "master": "t2.micro"
    },
    "tags": {
      "dev": "Key=Environment,Value=Development Key=env,Value=dev Key=appType,Value=Wordpress",
      "qa": "Key=Environment,Value=QA Key=env,Value=qa Key=appType,Value=Wordpress",
      "master": "Key=Environment,Value=Production Key=env,Value=live Key=appType,Value=Wordpress"
    },
    "sslCertificateId": "change-me",
    "s3Bucket": "change-me",
    "s3Delete": 1,
    "s3DeleteDaysOld": 7,
    "blueGreenDeployment": 0
  },
  "rds": {
    "instanceName": "change-me",
    "instanceClass": "db.m1.small",
    "vpcSecurityGroupIds": "change-me",
    "region": "us-east-1",
    "allocatedStorage": 40,
    "engine": "mysql",
    "engineVersion": "5.6.37"
  },
  "php": {
    "dev": {
      "memoryLimit": "256M",
      "outputCompression": "Off",
      "allowUrlFopen": "Off",
      "displayErrors": "On",
      "maxExecutionTime": 60
    },
    "qa": {
      "memoryLimit": "256M",
      "outputCompression": "On",
      "allowUrlFopen": "Off",
      "displayErrors": "Off",
      "maxExecutionTime": 60
    },
    "master": {
      "memoryLimit": "256M",
      "outputCompression": "On",
      "allowUrlFopen": "Off",
      "displayErrors": "Off",
      "maxExecutionTime": 60
    }
  },
  "basicAuth": {
    "dev": {
      "enabled": 1,
      "user": "change-me",
      "password": "change-me"
    },
    "qa": {
      "enabled": 1,
      "user": "change-me",
      "password": "change-me"
    },
    "master": {
      "enabled": 0,
      "user": "change-me",
      "password": "change-me"
    }
  }
}
